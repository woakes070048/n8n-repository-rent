    {
      "parameters": {
        "jsCode": "const items = $input.all();\nconst summary = {};\n\nfor (const item of items) {\n  const raw = item.json.data || item.json.text;\n  if (!raw) continue;\n\n  const lines = raw.split('\\n').filter(l => l.trim());\n  const headers = lines[0].split(',').map(h => h.trim());\n  const data = lines.slice(1);\n\n  for (const line of data) {\n    const fields = line.split(/,(?=(?:(?:[^\"]*\"){2})*[^\"]*$)/).map(f => f.replace(/^\"|\"$/g, '').trim());\n    const entry = Object.fromEntries(headers.map((h, i) => [h, fields[i]]));\n\n    const propId = parseInt(entry[\"Property ID\"]);\n    const source = entry[\"Source\"]?.trim() || \"Unattributed\";\n\n    if (!propId || isNaN(propId)) continue;\n\n    if (!summary[propId]) {\n      summary[propId] = {\n        property_id: propId,\n        guest_cards: 0,\n        lead_sources: {}\n      };\n    }\n\n    summary[propId].guest_cards += 1;\n    summary[propId].lead_sources[source] = (summary[propId].lead_sources[source] || 0) + 1;\n  }\n}\n\nreturn Object.values(summary).map(item => ({\n  json: {\n    ...item,\n    lead_sources: JSON.stringify(item.lead_sources).replace(/'/g, \"\\\\'\")\n  }\n}));\n"
      },

      "parameters": {
        "jsCode": "const fileData = $items(\"Code\")[0].json;\n\nconst rawFileName = fileData.name || \"Unknown File\";\nconst fileName = rawFileName.trim().toLowerCase();\nconst workflowName = $workflow.name || \"Unknown Workflow\";\nconst modifiedTime = fileData.modifiedTime || \"\"; // Might also be 'modifiedDate' in some cases\n\n// Map: lowercased sheet name → row number\nconst rowMap = {\n  \"n8n apartment delinquency daily\": 22,\n  \"n8n apartment guest cards past 7 days\": 23,\n  \"n8n apartment receivables to date\": 24,\n  \"n8n apartment receivables yesterday\": 25,\n  \"n8n apartment rent totals\": 26,\n  \"n8n apartment rental applications\": 27,\n  \"n8n apartment showings\": 28\n};\n\nconst row = rowMap[fileName];\n\nif (!row) {\n  throw new Error(`Unknown or unmapped file: ${fileName}`);\n}\n\nreturn [\n  {\n    json: {\n      row_number: row,\n      status: \"Success\",\n      workflow: workflowName,\n      file_name: fileName,\n      modified_time: modifiedTime,\n      error_message: \"\"\n    }\n  }\n];\n"
